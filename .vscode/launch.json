{
  "version": "0.2.0",
  "configurations": [
    

    {
      "type": "node",
      "request": "launch",
      "name": "Debug Express Server",
      "program": "${workspaceFolder}/server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeExecutable": "nodemon",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Loan Route File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/backend/src/routes/loan.js"
    }
  ]
}
